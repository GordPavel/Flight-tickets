            prefWidth="520.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.EditRoutesOverviewController">
    <children>
        <TextField fx:id="departureTextField" layoutX="170.0" layoutY="80.0" prefHeight="25.0" prefWidth="150.0"/>
        <Button layoutX="43.0" layoutY="244.0" mnemonicParsing="false" onAction="#handleEditAction" prefHeight="25.0" prefWidth="75.0"
                text="Accept" focusTraversable="false"/>
        <Label layoutX="95.0" layoutY="20.0" prefHeight="27.0" prefWidth="355.0"
