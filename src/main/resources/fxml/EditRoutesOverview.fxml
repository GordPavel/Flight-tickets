        </Label>
        <Button fx:id="cancelEditRoutesButton" layoutX="382.0" layoutY="244.0" mnemonicParsing="false"  onAction="#handleCancelAction" prefHeight="25.0" prefWidth="75.0"
                text="Cancel" />
        <TextField fx:id="destinationTextField" layoutX="170.0" layoutY="145.0" prefHeight="17.0" prefWidth="149.0"/>
        <Label layoutX="45.0" layoutY="85.0" prefHeight="17.0" prefWidth="64.0" text="Departure:">
            <font>
                <Font name="System Italic" size="12.0"/>
            </font>
        </Label>
        <Label layoutX="45.0" layoutY="150.0" text="Destination:">
            <font>
                <Font name="System Italic" size="12.0"/>
            </font>
        </Label>
        <Button layoutX="380.0" layoutY="145.0" mnemonicParsing="false" onAction="#clearData" prefHeight="25.0" prefWidth="75.0"
                text="Clear" focusTraversable="false"/>
    </children>
</AnchorPane>
