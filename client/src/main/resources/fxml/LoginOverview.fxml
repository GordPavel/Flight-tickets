<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="164.0" prefWidth="238.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1">
    <Label layoutX="23.0" layoutY="33.0" text="Login"/>
    <Label layoutX="23.0" layoutY="76.0" text="Password"/>
    <Button fx:id="logInButton" layoutX="28.0" layoutY="116.0" mnemonicParsing="false" onAction="#handleLogInAction"
            prefHeight="25.0" prefWidth="52.0" text="Log in"/>
    <Button fx:id="cancelButton" layoutX="154.0" layoutY="116.0" mnemonicParsing="false" onAction="#handleCancelAction"
            text="Cancel"/>
    <TextField fx:id="loginTextField" layoutX="79.0" layoutY="29.0" prefHeight="25.0" prefWidth="134.0"/>
    <PasswordField fx:id="passwordField" layoutX="80.0" layoutY="70.0" prefHeight="25.0" prefWidth="134.0"/>
</AnchorPane>
